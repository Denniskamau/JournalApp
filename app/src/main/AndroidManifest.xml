<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dennis.journalapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@drawable/girl"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".journalPage"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".WriteJournal">

        </activity>

        <provider
            android:authorities="com.example.dennis.journalapp"
            android:name=".data.JournalProvider"
            android:exported="false" />
    </application>

</manifest>